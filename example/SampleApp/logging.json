{
  "Logging": {
    "LogLevel": {
      "Default": "Critical"
    },
    "Elasticsearch": {
      "IncludeScopes": "true",
      "LogLevel": {
        "Default": "Error",
        "System": "Error",
        "Microsoft": "Error",
        "SampleApp.Program": "Error",
        "SampleApp.App": "Error"

      }
    },
    "Console": {
      "IncludeScopes": "true", "DisableColors" :  "true", 
      "LogLevel": {
        "Default": "Trace",
        "System": "Trace",
        "Microsoft": "Trace",
        "SampleApp.Program": "Trace",
        "SampleApp.App": "Trace"
      }
    }
  }
}